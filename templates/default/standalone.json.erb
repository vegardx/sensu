{
    "checks": {
        "check_dns": {
            "notifications": "DNS Serviceaddress"
            "occurrences": 4,
            "command": "/usr/lib/nagios/plugins/check_dig -l <%= node[:fqdn] %> -H 8.8.8.8 -a <%= node[:ipaddress] %>",
            "standalone": true,
            "interval": 60,
            "handlers": ["default"]
        }
    }
}
