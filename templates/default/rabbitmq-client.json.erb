<% @sensu_server.each do |sensu_server| -%>
{
  "rabbitmq": {
    "ssl": {
      "cert_chain_file": "/etc/sensu/ssl/cert.pem",
      "private_key_file": "/etc/sensu/ssl/key.pem"
    },
    "host": "<%= sensu_server['sensu']['host'] %>",
    "port": 5671,
    "vhost": "/sensu",
    "user": "<%= sensu_server['rabbitmq']['user'] %>",
    "password": "<%= sensu_server['rabbitmq']['password'] %>"
  }
}
<% end -%>
